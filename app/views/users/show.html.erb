<section "note-page">
<section class="note-view">



<h2>Profile</h2>

  <p><h3>Name: <%= current_user.name %></h3></p>

  <h2>My Gardens</h2>

  <ul>
    <% current_user.gardens.each do |garden| %>


      <h3 class="note-text"><%= link_to garden.name, garden_url(garden), method: :get %></h3>

      <br>

      <!-- <%= render "gardens/garden_info", garden: garden %> -->

    <% end %>
  </ul>

</section>
</section>

<%= button_to "Plant a New Garden!", new_garden_url, method: :get %>